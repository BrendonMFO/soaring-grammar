{
	"name": "Soaring Backend",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { 
			"VARIANT": "16"
		}
	},
	"extensions": [
		"eamodio.gitlens",
		"mikestead.dotenv",
		"dbaeumer.vscode-eslint",
		"wallabyjs.wallaby-vscode",
	],
	"runArgs": [
		"--name",
		"soaring-backend",
		"--network",
		"local",
		"-l",
		"traefik.frontend.rule=Host:soaring-backend.localhost",
		"-l",
		"traefik.port=5000",
		"-l",
		"traefik.docker.network=local"
	],
	"remoteUser": "node"
}